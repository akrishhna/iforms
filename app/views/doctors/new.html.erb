<% @doctornew = Doctor.where("user_id = ?",current_user.id).first %>
<% if !@doctornew %>
<div class="container">
<h3>Enter Profile Information</h3>
<%= render "form" %>
<%= link_to 'Back', doctors_path %>
</div>
<% else %>
<p>Your Profile Information is already in our database.</p>
<p>If you care to edit <%= link_to 'Click here', edit_profile_path(@doctornew)%><br><br>
<%= link_to 'Back', doctors_path %>
<%end%>

